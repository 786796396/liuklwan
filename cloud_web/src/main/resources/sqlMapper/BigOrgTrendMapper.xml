<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.BigOrgTrendDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.BigOrgTrend" id="resultMap"/>	<sql id="where">		<where>			1=1					</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.BigOrgTrend" useGeneratedKeys="true" keyProperty="id">		insert into big_org_trend ( 			<if test="id != null ">id,</if>			<if test="siteCode != null ">site_code,</if>			<if test="scanDate != null ">scan_date,</if>			<if test="healthindex != null ">healthindex,</if>			<if test="healthindexTrend != null ">healthindex_trend,</if>			<if test="linkerrsiteprop != null ">linkerrsiteprop,</if>			<if test="linkerrsitepropTrend != null ">linkerrsiteprop_trend,</if>			<if test="updatenum != null ">updatenum,</if>			<if test="updatenumTrend != null ">updatenum_trend,</if>			create_time,			modify_time		) values (			<if test="id != null ">#{id},</if>			<if test="siteCode != null ">#{siteCode},</if>			<if test="scanDate != null ">#{scanDate},</if>			<if test="healthindex != null ">#{healthindex},</if>			<if test="healthindexTrend != null ">#{healthindexTrend},</if>			<if test="linkerrsiteprop != null ">#{linkerrsiteprop},</if>			<if test="linkerrsitepropTrend != null ">#{linkerrsitepropTrend},</if>			<if test="updatenum != null ">#{updatenum},</if>			<if test="updatenumTrend != null ">#{updatenumTrend},</if>			now(),			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.BigOrgTrend">		select * from big_org_trend where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.BigOrgTrend">		update big_org_trend 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="siteCode != null ">site_code = #{siteCode},</if>				<if test="scanDate != null ">scan_date = #{scanDate},</if>				<if test="healthindex != null ">healthindex = #{healthindex},</if>				<if test="healthindexTrend != null ">healthindex_trend = #{healthindexTrend},</if>				<if test="linkerrsiteprop != null ">linkerrsiteprop = #{linkerrsiteprop},</if>				<if test="linkerrsitepropTrend != null ">linkerrsiteprop_trend = #{linkerrsitepropTrend},</if>				<if test="updatenum != null ">updatenum = #{updatenum},</if>				<if test="updatenumTrend != null ">updatenum_trend = #{updatenumTrend},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from big_org_trend where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from big_org_trend		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from big_org_trend		<include refid="where" />	</select>	
</mapper>
