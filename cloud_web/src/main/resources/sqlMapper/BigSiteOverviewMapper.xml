<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.BigSiteOverviewDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.BigSiteOverview" id="resultMap"/>	<sql id="where">		<where>			1=1			<if test="parentId != null ">				and parent_id=#{parentId} 			</if>			<if test="siteCode != null ">				and site_code=#{siteCode} 			</if>		</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.BigSiteOverview" useGeneratedKeys="true" keyProperty="id">		insert into big_site_overview ( 			<if test="id != null ">id,</if>			<if test="parentId != null ">parent_id,</if>			<if test="siteCode != null ">site_code,</if>			<if test="name != null ">name,</if>			<if test="sumClose != null ">sum_close,</if>			<if test="sumException != null ">sum_exception,</if>			<if test="sumNo != null ">sum_no,</if>			<if test="sumNormal != null ">sum_normal,</if>			<if test="sumReport != null ">sum_report,</if>			<if test="scanDate != null ">scan_date,</if>			create_time,			modify_time		) values (			<if test="id != null ">#{id},</if>			<if test="parentId != null ">#{parentId},</if>			<if test="siteCode != null ">#{siteCode},</if>			<if test="name != null ">#{name},</if>			<if test="sumClose != null ">#{sumClose},</if>			<if test="sumException != null ">#{sumException},</if>			<if test="sumNo != null ">#{sumNo},</if>			<if test="sumNormal != null ">#{sumNormal},</if>			<if test="sumReport != null ">#{sumReport},</if>			<if test="scanDate != null ">#{scanDate},</if>			now(),			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.BigSiteOverview">		select * from big_site_overview where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.BigSiteOverview">		update big_site_overview 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="parentId != null ">parent_id = #{parentId},</if>				<if test="siteCode != null ">site_code = #{siteCode},</if>				<if test="name != null ">name = #{name},</if>				<if test="sumClose != null ">sum_close = #{sumClose},</if>				<if test="sumException != null ">sum_exception = #{sumException},</if>				<if test="sumNo != null ">sum_no = #{sumNo},</if>				<if test="sumNormal != null ">sum_normal = #{sumNormal},</if>				<if test="sumReport != null ">sum_report = #{sumReport},</if>				<if test="scanDate != null ">scan_date = #{scanDate},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from big_site_overview where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from big_site_overview		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from big_site_overview		<include refid="where" />	</select>	
</mapper>
