<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.CfgOtherProductsDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.CfgOtherProducts" id="resultMap"/>	<sql id="where">		<where>			1=1			<if test="inUse != null ">				and in_use = #{inUse}			</if>			<if test="type != null ">				and type = #{type}			</if>		</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.CfgOtherProducts" useGeneratedKeys="true" keyProperty="id">		insert into cfg_other_products ( 			<if test="id != null ">id,</if>			<if test="icon != null ">icon,</if>			<if test="title != null ">title,</if>			<if test="description != null ">description,</if>			<if test="linkUrl != null ">link_url,</if>			<if test="inUse != null ">in_use,</if>			<if test="sort != null ">sort,</if>			<if test="specialShow != null ">special_show,</if>			<if test="isData != null ">is_data,</if>			<if test="remark != null ">remark,</if>			create_time,			modify_time,			<if test="createBy != null ">create_by,</if>			<if test="modifyBy != null ">modify_by,</if>		) values (			<if test="id != null ">#{id},</if>			<if test="icon != null ">#{icon},</if>			<if test="title != null ">#{title},</if>			<if test="description != null ">#{description},</if>			<if test="linkUrl != null ">#{linkUrl},</if>			<if test="inUse != null ">#{inUse},</if>			<if test="sort != null ">#{sort},</if>			<if test="specialShow != null ">#{specialShow},</if>			<if test="isData != null ">#{isData},</if>			<if test="remark != null ">#{remark},</if>			now(),			now(),			<if test="createBy != null ">#{createBy},</if>			<if test="modifyBy != null ">#{modifyBy},</if>		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.CfgOtherProducts">		select * from cfg_other_products where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.CfgOtherProducts">		update cfg_other_products 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="icon != null ">icon = #{icon},</if>				<if test="title != null ">title = #{title},</if>				<if test="description != null ">description = #{description},</if>				<if test="linkUrl != null ">link_url = #{linkUrl},</if>				<if test="inUse != null ">in_use = #{inUse},</if>				<if test="sort != null ">sort = #{sort},</if>				<if test="specialShow != null ">special_show = #{specialShow},</if>				<if test="isData != null ">is_data = #{isData},</if>				<if test="remark != null ">remark = #{remark},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime},</if>				<if test="createBy != null ">create_by = #{createBy},</if>				<if test="modifyBy != null ">modify_by = #{modifyBy},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from cfg_other_products where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from cfg_other_products		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from cfg_other_products		<include refid="where" />	</select>	
</mapper>
