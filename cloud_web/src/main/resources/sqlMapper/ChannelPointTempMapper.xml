<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.ChannelPointTempDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.ChannelPointTemp" id="resultMap"/>	<sql id="where">		<where>			1=1			<if test="siteCode != null">				and site_code = #{siteCode} 		    </if>			<if test="channelUrl != null">				and channel_url = #{channelUrl} 		    </if>		</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.ChannelPointTemp" useGeneratedKeys="true" keyProperty="id">		insert into channel_point_temp ( 			<if test="id != null ">id,</if>			<if test="siteCode != null ">site_code,</if>			<if test="dicChannelId != null ">dic_channel_id,</if>			<if test="dicChannelSonId != null ">dic_channel_son_id,</if>			<if test="channelName != null ">channel_name,</if>			<if test="channelUrl != null ">channel_url,</if>			<if test="status != null ">status,</if>			create_time,			modify_time		) values (			<if test="id != null ">#{id},</if>			<if test="siteCode != null ">#{siteCode},</if>			<if test="dicChannelId != null ">#{dicChannelId},</if>			<if test="dicChannelSonId != null ">#{dicChannelSonId},</if>			<if test="channelName != null ">#{channelName},</if>			<if test="channelUrl != null ">#{channelUrl},</if>			<if test="status != null ">#{status},</if>			now(),			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.ChannelPointTemp">		select * from channel_point_temp where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.ChannelPointTemp">		update channel_point_temp 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="siteCode != null ">site_code = #{siteCode},</if>				<if test="dicChannelId != null ">dic_channel_id = #{dicChannelId},</if>				<if test="dicChannelSonId != null ">dic_channel_son_id = #{dicChannelSonId},</if>				<if test="channelName != null ">channel_name = #{channelName},</if>				<if test="channelUrl != null ">channel_url = #{channelUrl},</if>				<if test="status != null ">status = #{status},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from channel_point_temp where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from channel_point_temp		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from channel_point_temp		<include refid="where" />	</select>	
</mapper>
