<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.ConfigAdvertDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.ConfigAdvert" id="resultMap"/>	<sql id="where">		<where>			1=1			<if test="id != null ">				and id = #{id} 			</if>			<if test="status != null ">				and status = #{status} 			</if>			<if test="adTime != null ">				and #{adTime} between start_time and end_time 			</if>		</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.ConfigAdvert" useGeneratedKeys="true" keyProperty="id">		insert into config_advert ( 			<if test="id != null ">id,</if>			<if test="name != null ">name,</if>			<if test="type != null ">type,</if>			<if test="adType != null ">ad_type,</if>			<if test="startTime != null ">start_time,</if>			<if test="endTime != null ">end_time,</if>			<if test="width != null ">width,</if>			<if test="height != null ">height,</if>			<if test="urlAddr != null ">url_addr,</if>			<if test="linkAddr != null ">link_addr,</if>			<if test="status != null ">status,</if>			<if test="remark != null ">remark,</if>			create_time,			modify_time		) values (			<if test="id != null ">#{id},</if>			<if test="name != null ">#{name},</if>			<if test="type != null ">#{type},</if>			<if test="adType != null ">#{adType},</if>			<if test="startTime != null ">#{startTime},</if>			<if test="endTime != null ">#{endTime},</if>			<if test="width != null ">#{width},</if>			<if test="height != null ">#{height},</if>			<if test="urlAddr != null ">#{urlAddr},</if>			<if test="linkAddr != null ">#{linkAddr},</if>			<if test="status != null ">#{status},</if>			<if test="remark != null ">#{remark},</if>			now(),			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.ConfigAdvert">		select * from config_advert where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.ConfigAdvert">		update config_advert 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="name != null ">name = #{name},</if>				<if test="type != null ">type = #{type},</if>				<if test="adType != null ">ad_type = #{adType},</if>				<if test="startTime != null ">start_time = #{startTime},</if>				<if test="endTime != null ">end_time = #{endTime},</if>				<if test="width != null ">width = #{width},</if>				<if test="height != null ">height = #{height},</if>				<if test="urlAddr != null ">url_addr = #{urlAddr},</if>				<if test="linkAddr != null ">link_addr = #{linkAddr},</if>				<if test="status != null ">status = #{status},</if>				<if test="remark != null ">remark = #{remark},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from config_advert where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from config_advert		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from config_advert		<include refid="where" />	</select>	
</mapper>
