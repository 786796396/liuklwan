<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.DicConfigDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.DicConfig" id="resultMap"/>	<sql id="where">		<where>			1=1					</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.DicConfig" useGeneratedKeys="true" keyProperty="id">		insert into dic_config ( 			<if test="id != null ">id,</if>			<if test="name != null ">name,</if>			<if test="value != null ">value,</if>			<if test="description != null ">description,</if>			<if test="status != null ">status,</if>			create_time,			modify_time		) values (			<if test="id != null ">#{id},</if>			<if test="name != null ">#{name},</if>			<if test="value != null ">#{value},</if>			<if test="description != null ">#{description},</if>			<if test="status != null ">#{status},</if>			now(),			now()		)	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.DicConfig">		select * from dic_config where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.DicConfig">		update dic_config 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="name != null ">name = #{name},</if>				<if test="value != null ">value = #{value},</if>				<if test="description != null ">description = #{description},</if>				<if test="status != null ">status = #{status},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime}</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from dic_config where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from dic_config		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from dic_config		<include refid="where" />	</select>	<select id="queryListByMap" parameterType="HashMap" resultType="com.ucap.cloud_web.dto.DicConfigRequest">		SELECT 		  * 		FROM		  dic_config 		WHERE 1=1 		<if test="configIds !=null">			and 			<foreach collection="configIds" item="item" open="(" separator="or" close=")">				id = #{item} 			</foreach>		</if>		order by id asc	</select>
</mapper>
