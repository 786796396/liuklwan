<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.DicInteractProbblemDao"><cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.DicInteractProbblem" id="resultMap"/>	<sql id="where">		<where>			1=1					</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.DicInteractProbblem" useGeneratedKeys="true" keyProperty="id">		insert into dic_interact_probblem ( 			<if test="id != null ">id,</if>			<if test="nteractProbblemCode != null ">nteract_probblem_code,</if>			<if test="remark != null ">remark,</if>			create_time,			modify_time		) values (			<if test="id != null ">#{id},</if>			<if test="nteractProbblemCode != null ">#{nteractProbblemCode},</if>			<if test="remark != null ">#{remark},</if>			now(),			now()		)	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.DicInteractProbblem">		select * from dic_interact_probblem where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.DicInteractProbblem">		update dic_interact_probblem 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="nteractProbblemCode != null ">nteract_probblem_code = #{nteractProbblemCode},</if>				<if test="remark != null ">remark = #{remark},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from dic_interact_probblem where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from dic_interact_probblem		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from dic_interact_probblem		<include refid="where" />	</select>	
</mapper>
