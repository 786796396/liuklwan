<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.EarlyLogDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.EarlyLog" id="resultMap"/>	<sql id="where">		<where>			1=1					</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.EarlyLog" useGeneratedKeys="true" keyProperty="id">		insert into early_log ( 			<!-- <if test="id != null ">id,</if> -->			<if test="siteCode != null ">site_code,</if>			<if test="servicePeriodId != null ">service_period_id,</if>			<if test="earlyType != null ">early_type,</if>			<if test="sendType != null ">send_type,</if>			<if test="contactAddress != null ">contact_address,</if>			<if test="content != null ">content,</if>			<if test="sendSquare != null ">send_square,</if>			create_time		) values (			<!-- <if test="id != null ">#{id},</if> -->			<if test="siteCode != null ">#{siteCode},</if>			<if test="servicePeriodId != null ">#{servicePeriodId},</if>			<if test="earlyType != null ">#{earlyType},</if>			<if test="sendType != null ">#{sendType},</if>			<if test="contactAddress != null ">#{contactAddress},</if>			<if test="content != null ">#{content},</if>			<if test="sendSquare != null ">#{sendSquare},</if>			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.EarlyLog">		select * from early_log where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.EarlyLog">		update early_log 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="siteCode != null ">site_code = #{siteCode},</if>				<if test="servicePeriodId != null ">service_period_id = #{servicePeriodId},</if>				<if test="earlyType != null ">early_type = #{earlyType},</if>				<if test="sendType != null ">send_type = #{sendType},</if>				<if test="contactAddress != null ">contact_address = #{contactAddress},</if>				<if test="content != null ">content = #{content},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="sendSquare != null ">send_square = #{sendSquare}</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from early_log where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from early_log		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from early_log		<include refid="where" />	</select>	
</mapper>
