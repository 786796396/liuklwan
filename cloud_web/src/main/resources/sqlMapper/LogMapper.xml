<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.LogDao"><cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.Log" id="resultMap"/>	<sql id="where">		<where>			1=1					</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.Log" useGeneratedKeys="true" keyProperty="id">		insert into log ( 			<if test="logId != null ">log_id,</if>			<if test="userId != null ">user_id,</if>			<if test="name != null ">name,</if>			log_date,			<if test="type != null ">type,</if>			<if test="mac != null ">mac,</if>			<if test="ip != null ">ip,</if>			<if test="objectType != null ">object_type,</if>			<if test="objectId != null ">object_id,</if>			<if test="eventName != null ">event_name,</if>			<if test="eventRecord != null ">event_record,</if>		) values (			<if test="logId != null ">#{logId},</if>			<if test="userId != null ">#{userId},</if>			<if test="name != null ">#{name},</if>			now(),			<if test="type != null ">#{type},</if>			<if test="mac != null ">#{mac},</if>			<if test="ip != null ">#{ip},</if>			<if test="objectType != null ">#{objectType},</if>			<if test="objectId != null ">#{objectId},</if>			<if test="eventName != null ">#{eventName},</if>			<if test="eventRecord != null ">#{eventRecord},</if>		)	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.Log">		select * from log where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.Log">		update log 			<trim prefix="SET" suffixOverrides=",">				<if test="logId != null ">log_id = #{logId},</if>				<if test="userId != null ">user_id = #{userId},</if>				<if test="name != null ">name = #{name},</if>				<if test="logDate != null ">log_date = #{logDate},</if>				<if test="type != null ">type = #{type},</if>				<if test="mac != null ">mac = #{mac},</if>				<if test="ip != null ">ip = #{ip},</if>				<if test="objectType != null ">object_type = #{objectType},</if>				<if test="objectId != null ">object_id = #{objectId},</if>				<if test="eventName != null ">event_name = #{eventName},</if>				<if test="eventRecord != null ">event_record = #{eventRecord},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from log where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from log		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from log		<include refid="where" />	</select>	</mapper>