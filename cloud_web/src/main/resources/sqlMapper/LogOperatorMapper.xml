<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.LogOperatorDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.LogOperator" id="resultMap"/>	<sql id="where">		<where>			1=1					</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.LogOperator" useGeneratedKeys="true" keyProperty="id">		insert into log_operator ( 			<if test="id != null ">id,</if>			<if test="siteCode != null ">site_code,</if>			<if test="contentsBefore != null ">contents_before,</if>			<if test="contentsAfter != null ">contents_after,</if>			<if test="type != null ">type,</if>			<if test="targetObject != null ">target_object,</if>			<if test="remark != null ">remark,</if>			<if test="operator != null ">operator,</if>			create_time		) values (			<if test="id != null ">#{id},</if>			<if test="siteCode != null ">#{siteCode},</if>			<if test="contentsBefore != null ">#{contentsBefore},</if>			<if test="contentsAfter != null ">#{contentsAfter},</if>			<if test="type != null ">#{type},</if>			<if test="targetObject != null ">#{targetObject},</if>			<if test="remark != null ">#{remark},</if>			<if test="operator != null ">#{operator},</if>			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.LogOperator">		select * from log_operator where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.LogOperator">		update log_operator 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="siteCode != null ">site_code = #{siteCode},</if>				<if test="contentsBefore != null ">contents_before = #{contentsBefore},</if>				<if test="contentsAfter != null ">contents_after = #{contentsAfter},</if>				<if test="type != null ">type = #{type},</if>				<if test="targetObject != null ">target_object = #{targetObject},</if>				<if test="remark != null ">remark = #{remark},</if>				<if test="operator != null ">operator = #{operator},</if>				<if test="createTime != null ">create_time = #{createTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from log_operator where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from log_operator		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from log_operator		<include refid="where" />	</select>	
</mapper>
