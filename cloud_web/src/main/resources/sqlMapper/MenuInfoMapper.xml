<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.MenuInfoDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.MenuInfo" id="resultMap"/>	<sql id="where">		<where>			1=1			<if test="menuType != null">				and menu_type = #{menuType}			</if>			<if test="positionType != null and positionType != 0">				and position_type = #{positionType}			</if>			<if test="isDisable != null">				and is_disable = #{isDisable}			</if>			<if test="parentId != null">				and parent_id = #{parentId}			</if>			<if test="onlyValue != null">				and only_value = #{onlyValue}			</if>		</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.MenuInfo" useGeneratedKeys="true" keyProperty="id">		insert into menu_info ( 			<if test="id != null ">id,</if>			<if test="parentId != null ">parent_id,</if>			<if test="parentIds != null ">parent_ids,</if>			<if test="isParent != null ">is_parent,</if>			<if test="name != null ">name,</if>			<if test="imgUrl != null ">img_url,</if>			<if test="gUrl != null ">g_url,</if>			<if test="menuType != null ">menu_type,</if>			<if test="positionType != null ">position_type,</if>			<if test="trialProduct != null ">trial_product,</if>			<if test="productType != null ">product_type,</if>			<if test="valueOf != null ">value_of,</if>			<if test="onlyValue != null ">only_value,</if>			<if test="orderNum != null ">order_num,</if>			<if test="isDisable != null ">is_disable,</if>			create_time,			modify_time,			<if test="createBy != null ">create_by,</if>			<if test="modifyBy != null ">modify_by,</if>		) values (			<if test="id != null ">#{id},</if>			<if test="parentId != null ">#{parentId},</if>			<if test="parentIds != null ">#{parentIds},</if>			<if test="isParent != null ">#{isParent},</if>			<if test="name != null ">#{name},</if>			<if test="imgUrl != null ">#{imgUrl},</if>			<if test="gUrl != null ">#{gUrl},</if>			<if test="menuType != null ">#{menuType},</if>			<if test="positionType != null ">#{positionType},</if>			<if test="trialProduct != null ">#{trialProduct},</if>			<if test="productType != null ">#{productType},</if>			<if test="valueOf != null ">#{valueOf},</if>			<if test="onlyValue != null ">#{onlyValue},</if>			<if test="orderNum != null ">#{orderNum},</if>			<if test="isDisable != null ">#{isDisable},</if>			now(),			now(),			<if test="createBy != null ">#{createBy},</if>			<if test="modifyBy != null ">#{modifyBy},</if>		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.MenuInfo">		select * from menu_info where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.MenuInfo">		update menu_info 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="parentId != null ">parent_id = #{parentId},</if>				<if test="parentIds != null ">parent_ids = #{parentIds},</if>				<if test="isParent != null ">is_parent = #{isParent},</if>				<if test="name != null ">name = #{name},</if>				<if test="imgUrl != null ">img_url = #{imgUrl},</if>				<if test="gUrl != null ">g_url = #{gUrl},</if>				<if test="menuType != null ">menu_type = #{menuType},</if>				<if test="positionType != null ">position_type = #{positionType},</if>				<if test="trialProduct != null ">trial_product = #{trialProduct},</if>				<if test="productType != null ">product_type = #{productType},</if>				<if test="valueOf != null ">value_of = #{valueOf},</if>				<if test="onlyValue != null ">only_value = #{onlyValue},</if>				<if test="orderNum != null ">order_num = #{orderNum},</if>				<if test="isDisable != null ">is_disable = #{isDisable},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime},</if>				<if test="createBy != null ">create_by = #{createBy},</if>				<if test="modifyBy != null ">modify_by = #{modifyBy},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from menu_info where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from menu_info		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from menu_info		<include refid="where" />	</select>	
</mapper>
