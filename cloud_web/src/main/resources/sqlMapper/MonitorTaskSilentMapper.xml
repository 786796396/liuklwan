<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.MonitorTaskSilentDao">	<!-- <cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>-->	<resultMap type="com.ucap.cloud_web.entity.MonitorTaskSilent" id="resultMap"/>	<sql id="where">		<where>			1=1			<if test="endTime !=null ">				 <![CDATA[ and date(end_date) <=  #{endTime} ]]>  			</if>			<if test="nowTime != null">			  	and #{nowTime} BETWEEN start_date and end_date 			</if>			<if test="endNowTime != null">				<![CDATA[ and  end_date <= #{endNowTime} ]]> 			</if>			<if test="siteCode != null">				and site_code = #{siteCode} 			</if>			<if test="contractIds != null">			 	and contract_id in            	<foreach collection="contractIds" item="term" index="index" open="(" separator="," close=")">       					${term}				</foreach>			</if>		</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.MonitorTaskSilent" useGeneratedKeys="true" keyProperty="id">		insert into monitor_task_silent ( 			<if test="id != null ">id,</if>			<if test="databaseInfoId != null ">database_info_id,</if>			<if test="siteCode != null ">site_code,</if>			<if test="encodeUrl != null ">encode_url,</if>			<if test="contractId != null ">contract_id,</if>			<if test="type != null ">type,</if>			start_date,			end_date,			<if test="isStart != null ">is_start,</if>			start_time,			<if test="rateTime != null ">rate_time,</if>			create_time,			modify_time		) values (			<if test="id != null ">#{id},</if>			<if test="databaseInfoId != null ">#{databaseInfoId},</if>			<if test="siteCode != null ">#{siteCode},</if>			<if test="encodeUrl != null ">#{encodeUrl},</if>			<if test="contractId != null ">#{contractId},</if>			<if test="type != null ">#{type},</if>			now(),			now(),			<if test="isStart != null ">#{isStart},</if>			now(),			<if test="rateTime != null ">#{rateTime},</if>			now(),			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.MonitorTaskSilent">		select * from monitor_task_silent where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.MonitorTaskSilent">		update monitor_task_silent 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="databaseInfoId != null ">database_info_id = #{databaseInfoId},</if>				<if test="siteCode != null ">site_code = #{siteCode},</if>				<if test="encodeUrl != null ">encode_url = #{encodeUrl},</if>				<if test="contractId != null ">contract_id = #{contractId},</if>				<if test="type != null ">type = #{type},</if>				<if test="startDate != null ">start_date = #{startDate},</if>				<if test="endDate != null ">end_date = #{endDate},</if>				<if test="isStart != null ">is_start = #{isStart},</if>				<if test="startTime != null ">start_time = #{startTime},</if>				<if test="rateTime != null ">rate_time = #{rateTime},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from monitor_task_silent where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from monitor_task_silent		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from monitor_task_silent		<include refid="where" />	</select>	
</mapper>
