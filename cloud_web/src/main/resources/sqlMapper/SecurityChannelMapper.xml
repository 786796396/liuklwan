<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.SecurityChannelDao"><cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.SecurityChannel" id="resultMap"/>	<sql id="where">		<where>			1=1			<if test="channelName != null ">				and channel_name like '%${channelName}%'			</if>		</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.SecurityChannel" useGeneratedKeys="true" keyProperty="id">		insert into security_channel ( 			<if test="id != null ">id,</if>			<if test="dicChannelId != null ">dic_channel_id,</if>			<if test="siteCode != null ">site_code,</if>			<if test="scanDate != null ">scan_date,</if>			<if test="channelName != null ">channel_name,</if>			<if test="url != null ">url,</if>			<if test="lastModifyTime != null ">last_modify_time,</if>			<if test="imgUrl != null ">img_url,</if>			create_time		) values (			<if test="id != null ">#{id},</if>			<if test="dicChannelId != null ">#{dicChannelId},</if>			<if test="siteCode != null ">#{siteCode},</if>			<if test="scanDate != null ">#{scanDate},</if>			<if test="channelName != null ">#{channelName},</if>			<if test="url != null ">#{url},</if>			<if test="lastModifyTime != null ">#{lastModifyTime},</if>			<if test="imgUrl != null ">#{imgUrl},</if>			now()		)	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.SecurityChannel">		select * from security_channel where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.SecurityChannel">		update security_channel 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="dicChannelId != null ">dic_channel_id = #{dicChannelId},</if>				<if test="siteCode != null ">site_code = #{siteCode},</if>				<if test="scanDate != null ">scan_date = #{scanDate},</if>				<if test="channelName != null ">channel_name = #{channelName},</if>				<if test="url != null ">url = #{url},</if>				<if test="lastModifyTime != null ">last_modify_time = #{lastModifyTime},</if>				<if test="imgUrl != null ">img_url = #{imgUrl},</if>				<if test="createTime != null ">create_time = #{createTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from security_channel where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from security_channel		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from security_channel		<include refid="where" />	</select>	
</mapper>
