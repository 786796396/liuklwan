<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.SpArticlePermissionDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.SpArticlePermission" id="resultMap"/>	<sql id="where">		<where>			1=1					</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.SpArticlePermission" useGeneratedKeys="true" keyProperty="id">		insert into sp_article_permission ( 			<if test="id != null ">id,</if>			<if test="articleId != null ">article_id,</if>			<if test="siteCode != null ">site_code,</if>			<if test="creater != null ">creater,</if>			create_time		) values (			<if test="id != null ">#{id},</if>			<if test="articleId != null ">#{articleId},</if>			<if test="siteCode != null ">#{siteCode},</if>			<if test="creater != null ">#{creater},</if>			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.SpArticlePermission">		select * from sp_article_permission where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.SpArticlePermission">		update sp_article_permission 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="articleId != null ">article_id = #{articleId},</if>				<if test="siteCode != null ">site_code = #{siteCode},</if>				<if test="creater != null ">creater = #{creater},</if>				<if test="createTime != null ">create_time = #{createTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from sp_article_permission where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from sp_article_permission		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from sp_article_permission		<include refid="where" />	</select>	
</mapper>
