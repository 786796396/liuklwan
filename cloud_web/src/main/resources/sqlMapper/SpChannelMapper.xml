<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.SpChannelDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.SpChannel" id="resultMap"/>	<sql id="where">		<where>			1=1			<if test="id != null and  id != 0">and id = #{id}</if>			<if test="notId != null and  notId != 0">and id != #{notId}</if>			<if test="delFlag != null ">and del_flag = #{delFlag}</if>		</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.SpChannel" useGeneratedKeys="true" keyProperty="id">		insert into sp_channel ( 			<if test="id != null ">id,</if>			<if test="clannelName != null ">clannel_name,</if>			<if test="delFlag != null ">del_flag,</if>			<if test="creater != null ">creater,</if>			create_time,			<if test="modiyer != null ">modiyer,</if>			modify_time		) values (			<if test="id != null ">#{id},</if>			<if test="clannelName != null ">#{clannelName},</if>			<if test="delFlag != null ">#{delFlag},</if>			<if test="creater != null ">#{creater},</if>			now(),			<if test="modiyer != null ">#{modiyer},</if>			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.SpChannel">		select * from sp_channel where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.SpChannel">		update sp_channel 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="clannelName != null ">clannel_name = #{clannelName},</if>				<if test="delFlag != null ">del_flag = #{delFlag},</if>				<if test="creater != null ">creater = #{creater},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modiyer != null ">modiyer = #{modiyer},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from sp_channel where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from sp_channel		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from sp_channel		<include refid="where" />	</select>	
</mapper>
