<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.SpSiteChannelDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.SpSiteChannel" id="resultMap"/>	<sql id="where">		<where>			1=1			<if test="siteCode != null ">				and site_code = #{siteCode}			</if>			<if test="sort != null ">				and sort = #{sort}			</if>			<if test="notId != null and  notId != 0">				and channel_id != #{notId}			</if>		</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.SpSiteChannel" useGeneratedKeys="true" keyProperty="id">		insert into sp_site_channel ( 			<if test="id != null ">id,</if>			<if test="channelId != null ">channel_id,</if>			<if test="siteCode != null ">site_code,</if>			<if test="sort != null ">sort,</if>			<if test="creater != null ">creater,</if>			create_time		) values (			<if test="id != null ">#{id},</if>			<if test="channelId != null ">#{channelId},</if>			<if test="siteCode != null ">#{siteCode},</if>			<if test="sort != null ">#{sort},</if>			<if test="creater != null ">#{creater},</if>			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.SpSiteChannel">		select * from sp_site_channel where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.SpSiteChannel">		update sp_site_channel 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="channelId != null ">channel_id = #{channelId},</if>				<if test="siteCode != null ">site_code = #{siteCode},</if>				<if test="sort != null ">sort = #{sort},</if>				<if test="creater != null ">creater = #{creater},</if>				<if test="createTime != null ">create_time = #{createTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from sp_site_channel where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from sp_site_channel		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from sp_site_channel		<include refid="where" />	</select>	
</mapper>
