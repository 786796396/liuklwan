<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ucap.cloud_web.dao.SpotSecurityReportDao">	<cache eviction="LRU" flushInterval="60000" size="1024" readOnly="true"/>	<resultMap type="com.ucap.cloud_web.entity.SpotSecurityReport" id="resultMap"/>	<sql id="where">		<where>			1=1			<if test="scheduleId != null and scheduleId != '' ">			    and spot_check_schedule_id = #{scheduleId}			</if>		</where>	</sql>	<!-- 添加 -->	<insert id="add" parameterType="com.ucap.cloud_web.entity.SpotSecurityReport" useGeneratedKeys="true" keyProperty="id">		insert into spot_security_report ( 			<if test="id != null ">id,</if>			<if test="spotCheckScheduleId != null ">spot_check_schedule_id,</if>			<if test="siteCode != null ">site_code,</if>			<if test="taskName != null ">task_name,</if>			<if test="startDate != null ">start_date,</if>			<if test="endDate != null ">end_date,</if>			<if test="type != null ">type,</if>			<if test="status != null ">status,</if>			<if test="spotWebsiteNum != null ">spot_website_num,</if>			<if test="affixUrl != null ">affix_url,</if>			<if test="affixName != null ">affix_name,</if>			<if test="reportOrgCode != null ">report_org_code,</if>			create_time,			modify_time		) values (			<if test="id != null ">#{id},</if>			<if test="spotCheckScheduleId != null ">#{spotCheckScheduleId},</if>			<if test="siteCode != null ">#{siteCode},</if>			<if test="taskName != null ">#{taskName},</if>			<if test="startDate != null ">#{startDate},</if>			<if test="endDate != null ">#{endDate},</if>			<if test="type != null ">#{type},</if>			<if test="status != null ">#{status},</if>			<if test="spotWebsiteNum != null ">#{spotWebsiteNum},</if>			<if test="affixUrl != null ">#{affixUrl},</if>			<if test="affixName != null ">#{affixName},</if>			<if test="reportOrgCode != null ">#{reportOrgCode},</if>			now(),			now()		)		<selectKey resultType="int" keyProperty="id">			SELECT LAST_INSERT_ID() AS id		</selectKey>	</insert>	<!-- 查询 -->	<select id="get" parameterType="int" resultType="com.ucap.cloud_web.entity.SpotSecurityReport">		select * from spot_security_report where id=#{id}	</select>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.SpotSecurityReport">		update spot_security_report 			<trim prefix="SET" suffixOverrides=",">				<if test="id != null ">id = #{id},</if>				<if test="spotCheckScheduleId != null ">spot_check_schedule_id = #{spotCheckScheduleId},</if>				<if test="siteCode != null ">site_code = #{siteCode},</if>				<if test="taskName != null ">task_name = #{taskName},</if>				<if test="startDate != null ">start_date = #{startDate},</if>				<if test="endDate != null ">end_date = #{endDate},</if>				<if test="type != null ">type = #{type},</if>				<if test="status != null ">status = #{status},</if>				<if test="spotWebsiteNum != null ">spot_website_num = #{spotWebsiteNum},</if>				<if test="affixUrl != null ">affix_url = #{affixUrl},</if>				<if test="affixName != null ">affix_name = #{affixName},</if>				<if test="reportOrgCode != null ">report_org_code = #{reportOrgCode},</if>				<if test="createTime != null ">create_time = #{createTime},</if>				<if test="modifyTime != null ">modify_time = #{modifyTime},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from spot_security_report where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from spot_security_report		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from spot_security_report		<include refid="where" />	</select>	
</mapper>
