ct>	<!-- 修改 -->	<update id="update" parameterType="com.ucap.cloud_web.entity.SystemCode">		update system_code 			<trim prefix="SET" suffixOverrides=",">				<if test="codeId != null ">code_id = #{codeId},</if>				<if test="codeMyid != null ">code_myid = #{codeMyid},</if>				<if test="name != null ">name = #{name},</if>				<if test="sort != null ">sort = #{sort},</if>				<if test="type != null ">type = #{type},</if>				<if test="iconcls != null ">iconcls = #{iconcls},</if>				<if test="state != null ">state = #{state},</if>				<if test="permissionid != null ">permissionid = #{permissionid},</if>				<if test="parentId != null ">parent_id = #{parentId},</if>				<if test="description != null ">description = #{description},</if>				<if test="status != null ">status = #{status},</if>				<if test="created != null ">created = #{created},</if>				<if test="lastmod != null ">lastmod = #{lastmod},</if>				<if test="creater != null ">creater = #{creater},</if>				<if test="modifyer != null ">modifyer = #{modifyer},</if>			</trim>			where id=#{id} 	</update>	<!-- 删除 -->	<delete id="delete" parameterType="int">		delete from system_code where id = #{id}	</delete>	<!-- 分页查询 -->	<select id="query" parameterType="HashMap" resultMap="resultMap">		select * from system_code		<include refid="where"/>		<if test="queryOrderList != null">			order by			<foreach collection="queryOrderList" item="queryOrder" separator=",">				${queryOrder}			</foreach>		</if>		<if test="start != null and pageSize != null">			limit #{start}, #{pageSize}		</if>	</select>	<select id="queryCount" parameterType="HashMap" resultType="int">		select count(*) from system_code		<include refid="where" />	</select>	
